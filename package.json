{
  "name": "peyman-me",
  "private": true,
  "description": "Peyman.me website",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build && gh-pages -d public -r https://github.com/p3yman/p3yman.github.io.git -b master",
    "develop": "env-cmd gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "dayjs": "^1.11.2",
    "gatsby": "^4.13.1",
    "gatsby-plugin-google-analytics": "^4.13.0",
    "gatsby-plugin-manifest": "^4.13.0",
    "gatsby-plugin-react-helmet": "^5.13.0",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sass": "^5.13.0",
    "gatsby-plugin-sharp": "^4.13.0",
    "gatsby-plugin-sitemap": "^5.13.0",
    "gatsby-remark-images": "^6.13.0",
    "gatsby-remark-prismjs": "^6.13.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^4.13.0",
    "gatsby-transformer-remark": "^5.13.0",
    "gh-pages": "^3.2.3",
    "prismjs": "^1.28.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.51.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "prettier": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peyman3d/peyman.me"
